{
  "name": "svelted-obsidian",
  "private": true,
  "version": "1.1.0",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "dev": "vite build --mode development --watch",
    "build": "vite build --mode production",
    "check": "svelte-check --tsconfig ./tsconfig.json",
