    "@types/node": "^20.8.10",
    "@unocss/extractor-svelte": "^0.57.2",
    "axios": "^1.13.2",
    "builtin-modules": "^3.3.0",
    "bun-types": "latest",
    "dotenv": "^17.2.3",
    "jsdom": "^27.4.0",
    "obsidian": "^1.4.11",
    "svelte": "^4.2.2",
    "svelte-check": "^3.5.2",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "unocss": "^0.57.2",
    "vite": "^4.5.0",
    "vitest": "^4.0.16"
  }
